{
  "_new_lines": "\n",
  "name": "my_selenide_pom_project",
  "repo_name": "{{ cookiecutter.name | lower | replace(' ', '_') }}",
  "package_name": "my.group.{{ cookiecutter.repo_name | replace('-', '') | replace('_', '') }}",
  "group": "{{ cookiecutter.package_name }}",
  "version": "0.0.1-SNAPSHOT",
  "selenide_pom_version": "0.18.7",
  "selenide_version": "6.15.0",
  "use_appium": ["yes", "no"],
  "use_cucumber": ["no", "yes"],
  "_copy_without_render": [
    ".idea/*",
    "gradle/*",
    "gradlew",
    "gradlew.bat",
    "gradle.properties",
    ".gitignore",
    "src/test/kotlin/*"
  ]
}
